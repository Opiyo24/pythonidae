'''USNG ELSE CLAUSE'''
try:
    age = int(input("Enter your age: "))
except ValueError:
    print("ERROR: Age cannot be ", end = "")
    print({}.format(age), end = "")
    print(", please enter a valid number")
else:
    if age<0:
        print("ERROR: Age cannot be negative.")
    else:
        print("Your age is: {:d}".format(age))